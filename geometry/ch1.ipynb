   ],
   "source": [
    "from sympy import * \n",
    "\n",
    "Va, Vp, Vpx, D, t = symbols('Va Vp Vpx D t') # Vpx is ratio Vp/Va\n",
    "\n",
    "solution = solve([\n",
    "    Vp - Va * Vpx,\n",
    "    Va * t + Vp * t - D\n",
    "], [Va, Vp])\n",
    "\n",
    "print(latex(solution))"
   ]
  }
